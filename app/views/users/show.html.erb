<h1>ユーザーID:<%= @user.id  %>の内容　</h1>
<%# <% if @user.id == @current_user.id %> %>
  <div class="card">
　  <div class="card-body">   
      <h5 class="card-title"><%= @user.user_name %></h5>
      <p class="card-text"><%= @user.nick_name %></p>
      <p class="card-text"><%= @user.birthday %></p>
      <p class="card-text"><%= @user.user_adress %></p>
      <p class="card-text"><%= @user.email %></p>
      <p class="card-text"><%= @user.user_gender %></p>
      <p class="card-text"><%= @user.password %></p>
      <p class="card-text"><%= @user.password_confirmation %></p>
      <p class="card-text"><%= @user.image_name %></p>
    </div>
  </div>　
  <% link_to("編集", "/users/#{@user.id}/edit") %>
<% end %>
